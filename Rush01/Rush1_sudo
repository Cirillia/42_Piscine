input = "4 3 2 1 1 2 2 2 4 3 2 1 1 2 2 2"
size_fo_input = 16	n
col_up = input[0-3]	0 ~ (n/4-1)
col_down = input[4-7]	n/4 ~ (n/2-1)
left = [8-11]		n/2 ~ (3/4 n -1)
right = [12]		3/4 n ~ (n-1)

check 4 - 1 - 3 - 2
if ==4:
	write 1234
if ==1:
	write 1
if ==3:
	if 4 already inside:
		put 3 in second place
	else:
		put 4 in third place